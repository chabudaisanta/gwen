{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "build_without_naming",
            "command": "/usr/bin/g++",
            "args": [
                "-fdiagnostics-color=always",
                "-Wall",
                "-Wextra",
                "-std=gnu++23",
                "-g",
                "${file}",
                "-o",
                "${workspaceFolder}/build/exe",
                "-I",
                "/home/chabudaisanta/lib/ac-library-master",
                "-I",
                "${workspaceFolder}",
                "-I",
                "/home/chabudaisanta/lib/gwen/src",
                "-DLOCAL",
                "-DCHABU",
                "-DCHABY",
                "-lgmpxx",
                "-lgmp",
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ],
    "version": "2.0.0"
}
